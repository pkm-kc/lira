<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="roundedImageViewRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeableImageViewStyle">
        <item name="strokeColor">@color/black</item>
        <item name="strokeWidth">2dp</item>
        <item name="android:padding">8dp</item>
    </style>

    <style name="ShapeableImageViewStyleWithDifferentPadding">
        <item name="strokeColor">@color/black</item>
        <item name="strokeWidth">2dp</item>
        <item name="android:padding">16dp</item>
    </style>

    <style name="ServiceButton">
        <item name="colorPrimary">@color/primary10</item>
    </style>

    <style name="TopBarTitleText">
        <item name="fontFamily">@font/sfprodisplaymedium</item>
        <item name="android:textSize">24sp</item>
        <item name="color">@color/white</item>
    </style>

</resources>